{
  "CertificateAuthority": {
    "Server": "your-ca-server.example.com",
    "Template": "WebServer",
    "DefaultCompany": "example.com",
    "DefaultOU": "IT"
  },
  "FilePaths": {
    "CertificateFolder": "%USERPROFILE%\\Documents\\Certificates",
    "LogPath": "%APPDATA%\\ZLGetCert\\Logs",
    "TempPath": "%TEMP%\\ZLGetCert"
  },
  "OpenSSL": {
    "ExecutablePath": "",
    "AutoDetect": true,
    "CommonPaths": [
      "C:\\Program Files\\OpenSSL-Win64\\bin\\openssl.exe",
      "C:\\Program Files (x86)\\OpenSSL-Win32\\bin\\openssl.exe",
      "C:\\OpenSSL-Win64\\bin\\openssl.exe",
      "C:\\OpenSSL-Win32\\bin\\openssl.exe"
    ]
  },
  "DefaultSettings": {
    "KeyLength": 2048,
    "HashAlgorithm": "sha256",
    "DefaultPassword": "",
    "RequirePasswordConfirmation": true,
    "AutoCleanup": true,
    "RememberPassword": false,
    "AvailableHashAlgorithms": ["sha256", "sha384", "sha512"]
  },
  "CertificateParameters": {
    "KeySpec": 1,
    "ProviderName": "Microsoft RSA SChannel Cryptographic Provider",
    "ProviderType": 12,
    "KeyUsage": "0xa0",
    "EnhancedKeyUsageOIDs": ["1.3.6.1.5.5.7.3.1"],
    "Exportable": true,
    "MachineKeySet": true,
    "SMIME": false,
    "PrivateKeyArchive": false,
    "UserProtected": false,
    "UseExistingKeySet": false
  },
  "Logging": {
    "LogLevel": "Information",
    "LogToFile": true,
    "LogToConsole": false,
    "MaxLogFileSize": "10MB",
    "MaxLogFiles": 5,
    "AvailableLogLevels": ["Trace", "Debug", "Information", "Warning", "Error", "Fatal"]
  }
}
