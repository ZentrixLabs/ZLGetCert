{
  "CertificateAuthority": {
    "Server": "your-ca-server.example.com",
    "Template": "WebServer",
    "DefaultCompany": "example.com",
    "DefaultOU": "IT"
  },
  "FilePaths": {
    "CertificateFolder": "C:\\ssl_dev",
    "LogPath": "C:\\ProgramData\\ZLGetCert\\Dev"
