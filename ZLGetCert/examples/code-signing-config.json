{
  "CertificateAuthority": {
    "Server": "ca.company.com",
    "Template": "CodeSigning",
    "DefaultCompany": "Software Company Inc.",
    "DefaultOU": "Development",
    "DefaultLocation": "Seattle",
    "DefaultState": "WA"
  },
  "FilePaths": {
    "CertificateFolder": "%USERPROFILE%\\Documents\\CodeSigning\\Certificates",
    "LogPath": "%APPDATA%\\ZLGetCert\\Logs",
    "TempPath": "%TEMP%\\ZLGetCert"
  },
  "DefaultSettings": {
    "KeyLength": 4096,
    "HashAlgorithm": "sha256",
    "DefaultPassword": "",
    "RequirePasswordConfirmation": true,
    "AutoCleanup": true,
    "RememberPassword": false,
    "AvailableHashAlgorithms": ["sha256", "sha384", "sha512"]
  },
  "CertificateParameters": {
    "KeySpec": 1,
    "ProviderName": "Microsoft RSA SChannel Cryptographic Provider",
    "ProviderType": 12,
    "KeyUsage": "0x80",
    "EnhancedKeyUsageOIDs": ["1.3.6.1.5.5.7.3.3"],
    "Exportable": true,
    "MachineKeySet": false,
    "SMIME": false,
    "PrivateKeyArchive": false,
    "UserProtected": true,
    "UseExistingKeySet": false
  },
  "Logging": {
    "LogLevel": "Information",
    "LogToFile": true,
    "LogToConsole": false,
    "MaxLogFileSize": "25MB",
    "MaxLogFiles": 7,
    "AvailableLogLevels": ["Trace", "Debug", "Information", "Warning", "Error", "Fatal"]
  }
}
