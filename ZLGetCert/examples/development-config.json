{
  "CertificateAuthority": {
    "Server": "dev-ca.local",
    "Template": "WebServer",
    "DefaultCompany": "Dev Company",
    "DefaultOU": "Development",
    "DefaultLocation": "San Francisco",
    "DefaultState": "CA"
  },
  "FilePaths": {
    "CertificateFolder": "%USERPROFILE%\\Documents\\Dev\\Certificates",
    "LogPath": "%APPDATA%\\ZLGetCert\\Logs",
    "TempPath": "%TEMP%\\ZLGetCert"
  },
  "DefaultSettings": {
    "KeyLength": 2048,
    "HashAlgorithm": "sha256",
    "DefaultPassword": "",
    "RequirePasswordConfirmation": true,
    "AutoCleanup": true,
    "RememberPassword": false,
    "AvailableHashAlgorithms": ["sha256", "sha384", "sha512"]
  },
  "CertificateParameters": {
    "KeySpec": 1,
    "ProviderName": "Microsoft RSA SChannel Cryptographic Provider",
    "ProviderType": 12,
    "KeyUsage": "0xa0",
    "EnhancedKeyUsageOIDs": ["1.3.6.1.5.5.7.3.1"],
    "Exportable": true,
    "MachineKeySet": false,
    "SMIME": false,
    "PrivateKeyArchive": false,
    "UserProtected": false,
    "UseExistingKeySet": false
  },
  "Logging": {
    "LogLevel": "Debug",
    "LogToFile": true,
    "LogToConsole": true,
    "MaxLogFileSize": "10MB",
    "MaxLogFiles": 5,
    "AvailableLogLevels": ["Trace", "Debug", "Information", "Warning", "Error", "Fatal"]
  }
}
